{
  "name": "unexpected-knex",
  "version": "1.0.0",
  "description": "Support for testing knex migrations using Unexpected",
  "main": "lib/unexpected-knex.js",
  "scripts": {
    "test": "nyc mocha test/**/*.spec.js",
    "coverage": "npm run test && nyc report --reporter=html && sensible-browser ./coverage/index.html"
  },
  "keywords": [
    "test",
    "testing",
    "migrations",
    "knex",
    "unexpected"
  ],
  "author": {
    "name": "Joel Mukuthu",
    "email": "joelmukuthu@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "unexpected-fs": "2.5.2"
  },
  "peerDependencies": {
    "knex": "0.12.0",
    "unexpected": "10.17.2"
  },
  "devDependencies": {
    "eslint": "3.5.0",
    "eslint-config-onelint": "1.1.0",
    "knex": "0.12.0",
    "mocha": "3.0.2",
    "nyc": "8.1.0",
    "unexpected": "10.17.2",
    "unexpected-fs": "2.5.2"
  },
  "nyc": {
    "all": true,
    "cache": true,
    "exclude": [
      "**/*.spec.js",
      "node_modules"
    ]
  }
}
