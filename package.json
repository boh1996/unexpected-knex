{
  "name": "unexpected-knex",
  "version": "0.0.1",
  "description": "Support for testing Knex.js models and migrations using Unexpected",
  "main": "lib/unexpected-knex.js",
  "scripts": {
    "lint": "eslint .",
    "test": "npm run lint && nyc mocha test/**/*.spec.js",
    "coverage": "npm run test && nyc report --reporter=html && sensible-browser ./coverage/index.html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "keywords": [
    "test",
    "testing",
    "knex",
    "database",
    "models",
    "migrations",
    "unexpected"
  ],
  "author": "Joel Mukuthu <joelmukuthu@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "knex": "0.12.0",
    "unexpected": "10.20.0"
  },
  "devDependencies": {
    "coveralls": "2.11.12",
    "dedent-js": "1.0.1",
    "eslint": "2.13.1",
    "eslint-config-onelint": "1.2.0",
    "knex": "0.12.0",
    "mocha": "3.0.2",
    "nyc": "8.1.0",
    "sqlite3": "3.1.4",
    "unexpected": "10.20.0",
    "unexpected-fs": "2.5.2",
    "unexpected-require": "2.0.0"
  },
  "nyc": {
    "cache": true,
    "include": [
      "lib/unexpected-knex.js"
    ]
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/unexpectedjs/unexpected-knex.git"
  },
  "bugs": {
    "url": "https://github.com/unexpectedjs/unexpected-knex/issues"
  },
  "homepage": "https://github.com/unexpectedjs/unexpected-knex#readme"
}
